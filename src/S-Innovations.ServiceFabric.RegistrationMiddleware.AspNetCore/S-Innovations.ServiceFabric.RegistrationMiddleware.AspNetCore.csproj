    <PackageReference Include="Serilog.Sinks.ApplicationInsights" Version="2.6.0" />
    <PackageReference Include="S-Innovations.ServiceFabric.Unity" Version="1.0.0-pre-2018052801" />
    <PackageReference Include="Microsoft.AspNetCore.HttpOverrides" Version="2.0.3" />
